.page-top{
    height: 100%;
    width: 100%;
    position: relative;
    z-index: 0;
    transition: All 0.4s ease;
    background-color: white;
    -webkit-transition: All 0.4s ease;
}
.c-state1{
    transform: rotate(0deg) scale(1) translate(100%,0%);
    -webkit-transform: rotate(0deg) scale(1) translate(100%,0%);
}
.page-bottom{
    height: 100%;
    width: 100%;
    position: fixed;
    z-index: 0;
    font-size: 13px;
    text-align: center;
    align-items: center;
}
.logo{
    width: 100%;
}
.picker-view-item{
    overflow-wrap: break-word
}