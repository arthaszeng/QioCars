        <view>
            <view class="brand-view">
                <scroll-view class="brand-scroll-view" scroll-y="true" scroll-into-view="{{wxSortPickerData.wxSortPickerViewtoView}}" scroll-top="0"
                             style="height: {{wxSortPickerData.view.scrollHeight}}px">
                    <block wx:for="{{wxSortPickerData.textData}}" wx:for-item="item" wx:key="">
                        <view id="{{item.tag}}" class="brand-picker-view-tag" wx:if="{{item.brandArray.length >0}}">{{item.tag}}</view>
                        <block wx:for="{{item.brandArray}}" wx:for-item="child" wx:key="">
                            <view class="brand-picker-view-item" catchtap="selectBrand" data-id="{{child.id}}">
                                <image class="brand-trademark" src="{{child.url}}"/>
                                <text class="brand-text">{{child.brandName}}</text>
                            </view>
                        </block>
                    </block>
                </scroll-view>
                <scroll-view class="wxSortPickerViewTemTags">
                    <block wx:for="{{charIndex}}" wx:for-item="item" wx:key="">
                        <view catchtap="wxSortPickerViewTemTagTap" data-tag="{{item}}" class="wxSortPickerViewTemTag">{{item}}</view>
                    </block>
                </scroll-view>
            </view>
            <picker-view indicator-class="indicator" class="car-picker-view" value="{{value}}" bindchange="bindChange">
                <picker-view-column class="picker-view-column">
                    <view class="picker-view-item" wx:for="{{queryData}}" wx:key="">{{item.name}}</view>
                </picker-view-column>
                <picker-view-column>
                    <view class="picker-view-item" wx:for="{{queryData[series].carlist}}" mode="aspectFit" wx:key="">{{item.name}}"</view>
                </picker-view-column>
                <picker-view-column bindtap="selectCar" class="picker-view-column">
                    <view class="picker-view-item-car" wx:for="{{queryData[series].carlist[subSeries].list}}" wx:key="">{{item.name}}
                    </view>
                </picker-view-column>
            </picker-view>
        </view>
